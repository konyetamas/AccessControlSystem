<Window x:Class="EntryManagement.View.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EntryManagement.View"
        mc:Ignorable="d"
        Title="TestWindow" Height="300" Width="300">
    <Grid>
        <ListView x:Name="listView" ItemsSource="{Binding TestList}" HorizontalAlignment="Left" Height="184" Margin="73,26,0,0" VerticalAlignment="Top" Width="145">
            <ListView.View>
                <GridView>
                    <GridViewColumn Width="30" Header="Id"
         DisplayMemberBinding="{Binding Id}"  />
                    <GridViewColumn Width="100" Header="First Name"  
         DisplayMemberBinding="{Binding Name}" />
                   
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
